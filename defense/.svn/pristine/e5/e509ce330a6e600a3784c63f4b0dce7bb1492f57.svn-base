	<select id="selectByMap" resultType="map" parameterType="map">
		select <foreach collection="alias" item="alia"
			index="index" separator=","> 
			<trim suffix="" suffixOverrides=",">
			${alia} ${index}
			</trim>
			</foreach>
		from
			${tableName}
			where mac=#{mac}
	</select>
	
	
	<select id="selectAll" resultType="map" parameterType="map">
		select <foreach collection="alias" item="alia"
			index="index" separator=","> 
			<trim suffix="" suffixOverrides=",">
			${alia} ${index}
			</trim>
			</foreach>
		from
			${tableName}
	</select>
	
	
	<delete id="deleteStations" parameterType="java.util.List">
		delete from 
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			${item.tableName}
		</foreach>
		where mac in
		<foreach item="item" index="index" collection="list" open="("
			separator="," close=")">
			#{item}
		</foreach>
	</delete>
  
  	<select id="selectAllByMac" parameterType="string" resultType="map">
		select mac, '保护终端' as tableName from station_trust WHERE mac=#{_parameter}
		union all
		select mac, '邻居终端' as tableName FROM station_neighbor WHERE mac=#{_parameter}
		union all 
		select mac,'访客终端' as tableName from station_visitor WHERE mac=#{_parameter}
		union all 
		select mac,'自组网终端' as tableName from station_ad_hoc WHERE mac=#{_parameter}
		union all 
		select mac,'自组网AP' as tableName from ap_ad_hoc WHERE mac=#{_parameter}
		union all 
		select mac,'邻居AP' as tableName from ap_neighbor WHERE mac=#{_parameter}
		union all 
		select mac,'信任AP' as tableName from ap_trust WHERE mac=#{_parameter}
		union all 
		select mac,'访客AP' as tableName from ap_visitor WHERE mac=#{_parameter}
		union all 
		select mac,'忽略MAC' as tableName from exception_mac WHERE mac=#{_parameter}
  	</select>
</mapper>